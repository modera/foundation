{
    "name": "modera/backend-security-bundle",
    "type": "symfony-bundle",
    "description": "Bundle provides UI section for MF that makes it possible to manage users/groups/permissions.",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergei Vizel"
        },
        {
            "name": "Sergei Lissovski"
        }
    ],
    "suggest": {
        "symfony/mailer": "^6.4|^7.0"
    },
    "conflict": {
        "symfony/mailer": "<6.4"
    },
    "require": {
        "php": ">=8.1",
        "modera/security-bundle": "^6.0",
        "modera/server-crud-bundle": "^6.0",
        "modera/translations-bundle": "^6.0",
        "modera/backend-tools-bundle": "^6.0",
        "modera/activity-logger-bundle": "^6.0",
        "modera/mjr-integration-bundle": "^6.0",
        "modera/backend-languages-bundle": "^6.0"
    },
    "require-dev": {
        "phake/phake": "^4.6",
        "phpunit/phpunit": "^10.0",
        "symfony/mailer": "^6.4|^7.0",
        "modera/foundation-bundle": "^6.0"
    },
    "autoload": {
        "psr-4": { "Modera\\BackendSecurityBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.x-dev"
        }
    }
}
