{
    "name": "modera/routing-bundle",
    "type": "symfony-bundle",
    "description": "This bundle makes it possible for bundles to dynamically include routing files from within your bundles.",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergei Vizel"
        },
        {
            "name": "Sergei Lissovski"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "^6.4|^7.0",
        "modera/expander-bundle": "^6.0"
    },
    "require-dev": {
        "phake/phake": "^4.6",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": { "Modera\\RoutingBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.x-dev"
        }
    }
}
