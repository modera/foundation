{
    "name": "modera/security-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "Provides database aware implementation of Security model + some other perks",
    "authors": [
        {
            "name": "Sergei Vizel"
        },
        {
            "name": "Sergei Lissovski"
        }
    ],
    "require": {
        "php": ">=8.1",
        "doctrine/orm": "^2.20",
        "doctrine/doctrine-bundle": "^2.14",
        "symfony/validator": "^6.4|^7.0",
        "symfony/security-bundle": "^6.4|^7.0",
        "symfony/framework-bundle": "^6.4|^7.0",
        "modera/expander-bundle": "^6.0",
        "modera/foundation-bundle": "^6.0",
        "modera/translations-bundle": "^6.0"
    },
    "require-dev": {
        "phake/phake": "^4.6",
        "phpunit/phpunit": "^10.0",
        "modera/foundation-bundle": "^6.0",
        "modera/languages-bundle": "^6.0"
    },
    "autoload": {
        "psr-4": { "Modera\\SecurityBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.x-dev"
        }
    }
}
