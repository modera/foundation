{
    "name": "modera/mjr-integration-bundle",
    "type": "symfony-bundle",
    "description": "Provides low level integration for MJR.",
    "license": "MIT",
    "homepage": "http://modera.org",
    "keywords": ["mjr"],
    "authors": [
        {
            "name": "Sergei Lissovski"
        },
        {
            "name": "Sergei Vizel"
        }
    ],
    "require": {
        "php": ">=8.1",
        "modera/direct-bundle": "^6.0",
        "modera/expander-bundle": "^6.0",
        "symfony/framework-bundle": "^6.4|^7.0",
        "symfony/yaml": "^6.4|^7.0"
    },
    "require-dev": {
        "phake/phake": "^4.6",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": { "Modera\\MjrIntegrationBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.x-dev"
        }
    }
}
