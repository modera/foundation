{
    "name": "modera/dynamically-configurable-mjr-bundle",
    "description": "Allows to configure some aspects of MJR integration using UIs.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergei Lissovski"
        },
        {
            "name": "Sergei Vizel"
        }
    ],
    "require": {
        "php": ">=8.1",
        "modera/backend-tools-settings-bundle": "^6.0",
        "modera/config-bundle": "^6.0",
        "modera/expander-bundle": "^6.0",
        "modera/mjr-integration-bundle": "^6.0"
    },
    "require-dev": {
        "phake/phake": "^4.6",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": { "Modera\\DynamicallyConfigurableMJRBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.x-dev"
        }
    }
}