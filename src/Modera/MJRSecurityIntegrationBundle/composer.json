{
    "name": "modera/mjr-security-integration-bundle",
    "type": "symfony-bundle",
    "description": "Bundle provides integration layer which is necessary to make MJR to be security aware.",
    "license": "MIT",
    "homepage": "http://modera.org",
    "keywords": ["mjr"],
    "authors": [
        {
            "name": "Sergei Lissovski"
        },
        {
            "name": "Sergei Vizel"
        }
    ],
    "require": {
        "php": ">=8.1",
        "doctrine/orm": "^2.20",
        "modera/expander-bundle": "^6.0",
        "modera/foundation-bundle": "^6.0",
        "modera/mjr-integration-bundle": "^6.0",
        "modera/security-bundle": "^6.0"
    },
    "require-dev": {
        "phake/phake": "^4.6",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": { "Modera\\MJRSecurityIntegrationBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.x-dev"
        }
    }
}
