{
    "name": "modera/backend-config-utils-bundle",
    "type": "symfony-bundle",
    "description": "Bundle provides tools that simplify contributing your own configuration sections to 'Backend/Tools/Settings' section.",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergei Lissovski"
        },
        {
            "name": "Sergei Vizel"
        }
    ],
    "require": {
        "php": ">=8.1",
        "modera/server-crud-bundle": "^6.0",
        "modera/dynamically-configurable-mjr-bundle": "^6.0"
    },
    "require-dev": {
        "phake/phake": "^4.6",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": { "Modera\\BackendConfigUtilsBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.x-dev"
        }
    }
}
